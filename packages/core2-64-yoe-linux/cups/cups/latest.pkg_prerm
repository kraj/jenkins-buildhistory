#!/bin/sh
set -e
if [ -n "$D" ]; then
	exit 0
fi

if type systemctl >/dev/null 2>/dev/null; then
	systemctl stop org.cups.cupsd.socket org.cups.cupsd.path org.cups.cupsd.service org.cups.cups-lpd.socket org.cups.cups-lpd@.service

	systemctl $OPTS disable org.cups.cupsd.socket org.cups.cupsd.path org.cups.cupsd.service org.cups.cups-lpd.socket org.cups.cups-lpd@.service
fi
