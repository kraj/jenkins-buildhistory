#!/bin/sh
set -e
if type systemctl >/dev/null 2>/dev/null; then
	if [ -z "$D" ]; then
		systemctl stop install_db.service

		systemctl disable install_db.service
	fi
fi
if [ -n "$D" -o ! -d /run/systemd/system ] && [ -z "$D" -a -x "/etc/init.d/install_db" ]; then
	/etc/init.d/install_db stop || :
fi
