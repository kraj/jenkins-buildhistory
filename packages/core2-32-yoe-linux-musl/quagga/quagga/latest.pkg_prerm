set -e
    /etc/init.d/quagga stop
if type systemctl >/dev/null 2>/dev/null; then
	if [ -z "$D" ]; then
		systemctl stop zebra.service

		systemctl disable zebra.service
	fi
fi
if [ -n "$D" -o ! -d /run/systemd/system ] && [ -z "$D" -a -x "/etc/init.d/quagga" ]; then
	/etc/init.d/quagga stop || :
fi
